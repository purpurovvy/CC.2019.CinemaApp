{
  "name": "cc.2019.cinemaapp",
  "version": "1.0.0",
  "description": "Simple cinema app made with express & react",
  "main": "index.js",
  "scripts": {
    "preinstall": "concurrently -n client,server \"cd client && npm i\" \"cd server && npm i\"",
    "lint": "eslint --ignore-path .gitignore --fix .",
    "format": "prettier --ignore-path .gitignore --write \"./**/*.{js,jsx,json,scss,html}\"",
    "dev": "concurrently -n client,server -k \"cd client && npm run start\" \"cd server && npm run start\"",
    "client": "cd client && npm run start",
    "server": "cd server && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/htk4/CC.2019.CinemaApp.git"
  },
  "author": "CodersCamp2019",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/htk4/CC.2019.CinemaApp/issues"
  },
  "homepage": "https://github.com/htk4/CC.2019.CinemaApp#readme",
  "devDependencies": {
    "concurrently": "^5.0.0",
    "create-react-app": "^3.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "mongoose": "^5.8.1",
    "morgan": "^1.9.1"
  }
}
